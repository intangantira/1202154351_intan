<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.intan_1202154351_modul1.MainActivity">

    <TextView
        android:id="@+id/text_makanMilea"
        android:layout_width="134dp"
        android:layout_height="53dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:text="@string/text_makanMilea"
        android:textAlignment="center"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="30dp" />

    <EditText
        android:id="@+id/porsi"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:layout_alignLeft="@+id/makanan"
        android:layout_alignStart="@+id/makanan"
        android:layout_below="@+id/makanan"
        android:layout_marginTop="17dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="195dp" />

    <EditText
        android:id="@+id/makanan"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:layout_alignLeft="@+id/btn_eat"
        android:layout_alignStart="@+id/btn_eat"
        android:layout_below="@+id/text_makanMilea"
        android:layout_marginTop="55dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="130dp" />

    <Button
        android:id="@+id/btn_eat"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn_ab"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:layout_toLeftOf="@+id/btn_ab"
        android:layout_toStartOf="@+id/btn_ab"
        android:background="#E91E63"
        android:onClick="launcheat"
        android:text="eatbus"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/btn_ab"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="199dp"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:background="#E91E63"
        android:onClick="launchab"
        android:text="abnormal"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="300dp" />

</RelativeLayout>
